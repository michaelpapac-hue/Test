{
  "name": "construction-daily-reporting-platform",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:backend": "npm --workspace @cdr/backend run dev",
    "dev:mobile": "npm --workspace @cdr/mobile run start",
